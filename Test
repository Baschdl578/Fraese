    <Script name_suffix="0" base_name="GoTo" name_space="__script__" type="Python" mobile="">
      <core>
		import VR
	
		VR.p1 = VR.Path()
		VR.p2 = VR.Path()
		VR.p3 = VR.Path()
	
		x_curr=0 #x-curr
		y_curr=0 #y-curr
		z_curr=0 #z-curr
	
		speed=1
		x_next=0.7 #x-next   #0.7
		y_next=-0.21 #y-next   #-0.21
		z_next=0.55 #z-next   #0.55
	
	
	
		VR.p1.set([x_curr,0,0], [0,0,-1], [x_next,0,0], [0,0,-1], 2)
		VR.p2.set([x_curr,0,z_curr], [0,0,-1], [x_next,0,z_next], [0,0,-1], 2)
		VR.p3.set([x_curr,y_curr,z_curr], [0,0,-1], [x_next,y_next,z_next], [0,0,-1], 2)
	
	
		for obj in VR.getRoot().find("Arm1").getChildren(True):
			obj.animate(VR.p1, speed, 0, True)
			
		for obj in VR.getRoot().find("Arm2").getChildren(True):
			obj.animate(VR.p2, speed, 0, True)
		
		for obj in VR.getRoot().find("Motor").getChildren(True):
			obj.animate(VR.p3, speed, 0, True)
</core>
    </Script>
    <Script name_suffix="0" base_name="ReadFile" name_space="__script__" type="Python" mobile="">
      <core>

	def LineManipulation(line):
		import VR
		lone=line
		
		VR.triggerScript(GoTo(0, 0, 0, 0.7, -0.21, 0.55))
		#GoTo(0, 0, 0, 0.7, -0.21, 0.55)
		
		
	

	fname="/home/baschdl/g_code_Example"
	
	with open(fname) as f:
		line = f.readlines()
		
		if line[0]!="%":
			LineManipulation(line)
			
			

</core>
    </Script>
  </Scripts>
